<mui:ModernWindow x:Class="XIVRecord.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:v="clr-namespace:XIVRecord.Views"
        xmlns:mui="http://firstfloorsoftware.com/ModernUI"
        Title="MainWindow" Height="350" Width="525"
        Loaded="ModernWindow_Loaded">
    <mui:ModernWindow.MenuLinkGroups>
        <mui:LinkGroup DisplayName="Archive">
            <mui:LinkGroup.Links>
                <mui:Link DisplayName="All" Source="/Views/ArchiveDirView.xaml" />
            </mui:LinkGroup.Links>
        </mui:LinkGroup>
    </mui:ModernWindow.MenuLinkGroups>
    <mui:ModernFrame x:Name="frame" Style="{StaticResource ContentRoot}" DataContext="{Binding ContentContext}">
    </mui:ModernFrame>
</mui:ModernWindow>
